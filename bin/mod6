    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x561c5c72b230_0;
    %pad/u 32;
    %cmpi/u 1, 0, 32;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %assign/vec4 v0x561c5c72b2f0_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x561c5c72b230_0;
    %pad/u 32;
    %cmpi/u 0, 0, 32;
    %flag_get/vec4 4;
    %flag_get/vec4 5;
    %or;
    %assign/vec4 v0x561c5c72b2f0_0, 0;
T_0.5 ;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x561c5c72ae00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x561c5c72b150_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561c5c72b230_0, 0;
