    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x561c5c72b040_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x561c5c72aec0_0;
    %assign/vec4 v0x561c5c72b150_0, 0;
    %load/vec4 v0x561c5c72aec0_0;
