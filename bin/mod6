    %jmp T_0.9;
T_0.8 ;
    %load/vec4 v0x561c5c72b150_0;
    %pad/u 32;
    %subi 1, 0, 32;
    %pushi/vec4 6, 0, 32;
    %mod;
    %pad/u 4;
    %assign/vec4 v0x561c5c72b150_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561c5c72b230_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561c5c72b2f0_0, 0;
T_0.9 ;
T_0.7 ;
T_0.3 ;
T_0.1 ;
